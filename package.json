{
  "name": "threejs-sandbox",
  "version": "0.0.0",
  "description": "threejs sandbox to learn",
  "private": true,
  "scripts": {
    "clear": "rimraf dist",
    "prebuild": "npm run clear && npm install",
    "build": "webpack --mode production --colors --progress",
    "serve:compiled": "tiny-server ./dist",
    "start": "webpack-dev-server --mode development --hot --colors --progress --open --history-api-fallback",
    "stats:build:dev": "webpack --mode development --config webpack.config.js --profile --json > stats.json",
    "stats:build:prod": "webpack --mode production --config webpack.config.js --profile --json > stats.json",
    "stats:view": "webpack-bundle-analyzer stats.json",
    "stats": "npm run stats:build:dev && npm run stats:view",
    "stats:prod": "npm run stats:build:prod && npm run stats:view",
    "pretest": "rimraf coverage",
    "test": "npm run eslint & npm run units",
    "units": "karma start",
    "units:watch": "npm run units -- --single-run=false --auto-watch=true --reporters=progress",
    "eslint": "eslint --color --cache --ext .js,.jsx,.ts,.tsx src",
    "eslint:dump": "eslint --print-config ./.eslintrc.json"
  },
  "author": "blindlybright",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.6.5",
    "es6-promise": "^4.2.8",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "three": "^0.116.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "@types/stats.js": "^0.17.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "ignore-loader": "^0.1.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^5.0.4",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.2.3",
    "mocker-api": "^1.14.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "node-sass": "^4.14.0",
    "null-loader": "^3.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.3.1",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.5.0",
    "stats.js": "^0.17.0",
    "style-loader": "^0.23.1",
    "tiny-server": "^1.1.0",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "uglify-js-es6": "^2.8.9",
    "uglifyes-webpack-plugin": "^0.4.3",
    "url-loader": "^2.3.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "browserslist": [
    "last 2 version",
    "ie >= 10",
    "> 1%"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
